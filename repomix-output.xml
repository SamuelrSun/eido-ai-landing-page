@tailwind base;
@tailwind components;
@tailwind utilities;

@layer base {
  :root {
    --background: 240 10% 3.9%;
    /* Dark background */
    --foreground: 0 0% 98%;
    /* Light text */

    --card: 240 10% 3.9%;
    --card-foreground: 0 0% 98%;

    --popover: 240 10% 3.9%;
    --popover-foreground: 0 0% 98%;

    --primary: 240 5.9% 10%; /* Eido charcoal for buttons */
    --primary-foreground: 0 0% 98%;
    /* Light text on buttons */

    --secondary: 240 4.8% 95.9%;
    --secondary-foreground: 240 5.9% 10%;

    --muted: 240 3.7% 15.9%;
    --muted-foreground: 240 5% 64.9%;

    --accent: 240 3.7% 15.9%;
    --accent-foreground: 0 0% 98%;

    --destructive: 0 62.8% 30.6%;
    --destructive-foreground: 0 0% 98%;

    --border: 240 3.7% 15.9%; /* Lighter border for dark mode */
    --input: 240 3.7% 15.9%;
    --ring: 240 4.9% 83.9%;

    --radius: 0.5rem;
  }
}

@layer base {
  * {
    @apply border-border;
  }
  body {
    @apply bg-background text-foreground;
  }

  /* --- GRAINY BACKGROUND EFFECT (REVISED) --- */
  body::before {
    content: "";
    position: fixed;
    top: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
    z-index: -1;
    pointer-events: none;
    /* Base64 encoded noise PNG, repeated over the screen */
    background: url('data:image/png;base64,');
    animation: grain 0.5s steps(1) infinite;
  }

  @keyframes grain {
    0%, 100% { transform: translate(0, 0); }
    10% { transform: translate(-1%, -2%); }
    20% { transform: translate(-3%, 1%); }
    30% { transform: translate(1%, -3%); }
    40% { transform: translate(-1%, 3%); }
    50% { transform: translate(-3%, 2%); }
    60% { transform: translate(3%, 0%); }
    70% { transform: translate(0%, 3%); }
    80% { transform: translate(1%, 4%); }
    90% { transform: translate(-2%, 2%); }
  }
  /* --- END GRAINY BACKGROUND EFFECT --- */
}

/* Add these new styles for the shimmer effect */
@layer utilities {
  .shimmer-button {
    @apply relative overflow-hidden transition-colors duration-300;
  }

  /* Define the spotlight pseudo-element */
  .shimmer-button::before {
    content: "";
    position: absolute;
    inset: 0;
    border-radius: inherit; /* Match the button's border radius */

    /* The radial gradient for the spotlight effect */
    background: radial-gradient(
      80px circle at var(--x) var(--y),
      rgba(96, 165, 250, 0.25), /* Tailwind's blue-400 with 25% opacity */
      transparent 70%
    );
    opacity: 0;
    transition: opacity 0.3s ease-in-out;
    pointer-events: none; /* Ensure it doesn't interfere with clicks */
  }

  /* Show the spotlight on hover */
  .shimmer-button:hover::before {
    opacity: 1;
  }
  
  /* Custom utility to hide scrollbars */
  .no-scrollbar::-webkit-scrollbar {
    display: none;
  }
  .no-scrollbar {
    -ms-overflow-style: none;  /* IE and Edge */
    scrollbar-width: none;  /* Firefox */
  }

  /* --- FLIP ANIMATION STYLES --- */
  .flip-container {
    perspective: 1000px;
    /* Creates the 3D space */
    cursor: pointer;
  }

  .flipper {
    position: relative;
    transform-style: preserve-3d;
    transition: transform 0.6s;
  }

  .flip-container:hover .flipper {
    transform: rotateX(-180deg);
  }

  .front,
  .back {
    display: block;
    backface-visibility: hidden;
    /* Hides the back of the element when flipped */
    position: relative;
    padding: 2px 0;
  }

  .back {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    transform: rotateX(180deg);
  }
  /* --- END FLIP ANIMATION STYLES --- */
}
